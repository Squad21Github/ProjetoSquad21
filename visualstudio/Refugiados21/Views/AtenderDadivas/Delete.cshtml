@model Refugiados1.Models.AtenderDadiva

@{
    ViewData["Title"] = "Delete";
}
<header>
    <div>
        <h4>Excluir</h4>

        <h5>Tem certeza que deseja excluir?</h5>
    </div>
</header>

<section class="section">
    <br />
    <h4>Atender Dádiva</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Seu Nome
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Seu_Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Telefone)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Telefone)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CPF)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CPF)
        </dd>
        <dt class = "col-sm-2">
           Dádiva
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SolicitarDadiva.Dádiva)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdAtender" />
        <input type="submit" value="Excluir" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-primary">Voltar</a>
    </form>
</section>
